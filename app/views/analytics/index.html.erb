<h1>HELLO ANALYSIS</h1>
<%= column_chart @assessments.analysis_by_assessment %>
<ul>
  <% @students.sort_by {|student| student.last_assessment.section_number }.each do |student| %>
    <li><%= student.name %> - <%= student.last_assessment.title %></li>
  <% end %>
</ul>
<%= bar_chart @students.students_by_assessment %>
<% if false %>
<%= line_chart @goals.map{|goal|
    {name: goal.name, data: goal.feats.group_by_week(:created_at).count}
} %>
[{name: goal.name, data: {date1 => count, date2 => count}}]
<% end %>
